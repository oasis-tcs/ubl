<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            targetNamespace=
     "urn:oasis:names:specification:ubl:schema:xsd:CommonSignatureComponents-2"
            xmlns=
     "urn:oasis:names:specification:ubl:schema:xsd:CommonSignatureComponents-2"
            xmlns:sig=
     "urn:oasis:names:specification:ubl:schema:xsd:CommonSignatureComponents-2"
            xmlns:sac=
  "urn:oasis:names:specification:ubl:schema:xsd:SignatureAggregateComponents-2"
            xmlns:ccts="urn:un:unece:uncefact:documentation:2"
            elementFormDefault="qualified" 
            attributeFormDefault="unqualified" 
            version="2.4">

   <!-- ===== Import ===== -->
   <xsd:import schemaLocation="UBL-SignatureAggregateComponents-2.4.xsd" 
               namespace=
"urn:oasis:names:specification:ubl:schema:xsd:SignatureAggregateComponents-2"/>

   <!-- ===== Element Declarations ===== -->
   <xsd:element name="UBLDocumentSignatures" 
                type="UBLDocumentSignaturesType"/>

   <!-- ===== Type Definitions ===== -->
   <xsd:complexType name="UBLDocumentSignaturesType">
     <xsd:annotation>
       <xsd:documentation>
         <ccts:Definition>This class collects all signature information for a document.</ccts:Definition>
       </xsd:documentation>
     </xsd:annotation>
     <xsd:sequence>
       <xsd:element ref="sac:SignatureInformation" 
                    minOccurs="1" maxOccurs="unbounded">
         <xsd:annotation>
           <xsd:documentation>
             <ccts:Definition>Each of these is scaffolding for a single digital signature.</ccts:Definition>
           </xsd:documentation>
         </xsd:annotation>
       </xsd:element>
     </xsd:sequence>
   </xsd:complexType>
   
</xsd:schema>
